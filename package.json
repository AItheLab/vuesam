{
  "name": "vue-sam",
  "version": "0.0.1",
  "description": "A modern Vue 3 component library",
  "type": "module",
  "main": "./dist/vue-sam.umd.js",
  "module": "./dist/vue-sam.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue-sam.es.js",
      "require": "./dist/vue-sam.umd.js"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@storybook/addon-themes": "^8.0.4",
    "@vueuse/core": "^10.7.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@storybook/vue3": "^8.0.4",
    "@storybook/vue3-vite": "^8.6.14",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "happy-dom": "^13.3.1",
    "prettier": "^3.2.4",
    "sass": "^1.77.8",
    "storybook": "^8.0.4",
    "typescript": "~5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.1",
    "vitepress": "^1.0.0-rc.39",
    "vitest": "^1.2.1",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vue-sam.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "library",
    "ui",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/vue-sam/issues"
  },
  "homepage": "https://github.com/yourusername/vue-sam#readme"
}
